<h1>testing</h1>
<%= form_for(@user) do |form|%>

  <div class="field">
    <%= form.label :name%>
    <%= form.text_field :name, id: :user_name%>
  </div>

  <div class="field">
    <%= form.label :email%>
    <%= form.email_field :email, id: :user_email%>
  </div>

  <fieldset>
      <p>Leave these fields blank if you don't want to change your password.</p>
      <div class="form-group">
        <%= form.label :password %>
        <%= form.password_field :password, :autocomplete => 'off', class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= form.label :password_confirmation %>
        <%= form.password_field :password_confirmation, class: 'form-control' %>
      </div>
    </fieldset>
  <div class = "actions">
    <%= form.submit%>
  </div >
<% end %>